{
    "defaultStacks": ["frontend"],
    "packages":{
        "frontend-static":{
            "type":"aws-s3",
            "data":{
                "bucket":"aws-frontend-static",
                "cacheControl":"public, max-age=315360000"
            }
        }
    },
    "recipes":{
        "default":{
            "depends" : ["staticFilesUpload"]
        },
        "staticFilesUpload":{
            "actionsBeforeApp": ["frontend-static.uploadStaticFiles"]
        }
    }
}
